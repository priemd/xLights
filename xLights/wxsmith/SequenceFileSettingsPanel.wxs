				<row>4</row>
				<flag>wxALL|wxALIGN_LEFT|wxALIGN_CENTER_VERTICAL</flag>
				<border>5</border>
				<option>1</option>
			</object>
			<object class="sizeritem">
				<object class="wxChoice" name="ID_CHOICE2" variable="AutoSaveIntervalChoice" member="yes">
					<content>
						<item>Disabled</item>
						<item>3 Minutes</item>
						<item>5 Minutes</item>
						<item>10 Minutes</item>
						<item>15 Minutes</item>
						<item>30 Minutes</item>
